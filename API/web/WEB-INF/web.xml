<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tabuada</servlet-name>
        <servlet-class>Tabuada</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tabuada2</servlet-name>
        <servlet-class>Tabuada2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Autentica</servlet-name>
        <servlet-class>Autentica</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Usuarios</servlet-name>
        <servlet-class>Usuarios</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tabuada</servlet-name>
        <url-pattern>/Tabuada</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tabuada2</servlet-name>
        <url-pattern>/Tabuada2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Autentica</servlet-name>
        <url-pattern>/Autentica</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Usuarios</servlet-name>
        <url-pattern>/Usuarios</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
